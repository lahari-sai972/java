import java.util.*;
class Palindrome
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter n value:");
        int n=sc.nextInt();
        int sum =0;
        int a;
        int temp=n;
        while(n>0)
        {
            a=n%10;
            sum=sum*10+a;
	    n=n/10;

        }
        if(sum==temp)
            System.out.println(n+"is a palindrome");
        else
            System.out.println(n+"is not a palindrome");  

    }
}
